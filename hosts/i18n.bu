variant: fcos
version: 1.4.0
storage:
  files:
    # Set default keymap to de
    - path: /etc/vconsole.conf
      overwrite: true
      contents:
        inline: |
          KEYMAP=de
  # Set TZ to EU/Vienna
  links:
    - path: /etc/localtime
      overwrite: true
      target: /usr/share/zoneinfo/Europe/Vienna
      hard: false
